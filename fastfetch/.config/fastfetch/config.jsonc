{
  "$schema": "https://github.com/fastfetch-cli/fastfetch/raw/dev/doc/json_schema.json",
  "logo": {
    "source": "arch2",
    "color": {"1": "green", "2": "green"},
    "type": "auto",
    "padding": {
      "top": 0,
      "left": 4,
      "right": 8
    }
  },
  "display": {
    "key": {
      "width": 20
    },
    "separator": "  →  ",
    "color": {
      "keys": "default",
      "separator": "magenta"
    }
  },
  "modules": [
    "break",
    {
      "key": "Distro    ",
      "type": "os",
      "format": "Arch Linux"
    },
    {
      "key": "Kernel    ",
      "type": "kernel"
    },
    {
      "key": "CPU       ",
      "type": "cpu",
      "temp": true
    },
    {
      "key": "GPU       ",
      "type": "gpu",
      "temp": true
    },
    "break",
    {
      "key": "Desktop   ",
      "type": "de"
    },
    {
      "key": "Manager   ",
      "type": "wm"
    },
    {
      "key": "Shell     ",
      "type": "shell"
    },
    {
      "key": "Terminal  ",
      "type": "terminal"
    },
    "break",
    {
      "key": "Disk      ",
      "type": "disk"
    },
    {
      "key": "Memory    ",
      "type": "memory"
    },
    {
      "key": "Network   ",
      "type": "localip"
    },
    {
      "key": "Uptime    ",
      "type": "uptime"
    },
    "break",
    {
      "key": "Packages  ",
      "type": "packages"
    },
    "break",
    {
      "type": "custom",
      "format": "{#red}●    {#green}●    {#yellow}●    {#blue}●    {#magenta}●    {#cyan}●    {#white}●    {#default}●"
    }
  ]
}
